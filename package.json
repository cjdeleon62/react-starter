{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "rimraf cov && npm run test",
    "prod": "npm run build && node server/index.js",
    "clean": "rimraf dist",
    "dev": "npm run clean && webpack-dev-server --mode development --hot --env.NODE_ENV=development",
    "lint": "eslint **/*.js --ignore-pattern node_modules/",
    "build": "npm run clean && webpack --mode production"
  },
  "repository": "git+https://github.com/cjdeleon62/react-starter.git",
  "author": "Christopher De Leon <cdeleon@forbes.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cjdeleon62/react-starter/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "collectCoverage": true,
    "coverageDirectory": "cov",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "homepage": "https://github.com/cjdeleon62/react-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "autoprefixer": "^8.6.0",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  }
}
